enum JobStatus {
  ACTIVE
  ARCHIVED
}

type Board {
  uuid: ID!
  title: String!
  jobs: [Job!]
  createdAt: DateTime!
  updatedAt: DateTime!
  isDeleted: Boolean!
}

type Job {
  uuid: ID!
  title: String!
  status: JobStatus!
  createdAt: DateTime!
  updatedAt: DateTime!
  isDeleted: Boolean!
}
