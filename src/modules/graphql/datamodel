enum BoardStatus {
  ACTIVE
  ARCHIVED
}

enum JobStatus {
  ACTIVE
  ARCHIVED
}

type Board {
  uuid: ID!
  title: String!
  status: BoardStatus!
  jobs: [Job!]
  createdAt: DateTime!
  updatedAt: DateTime!
}

type Job {
  uuid: ID!
  createdAt: DateTime!
  updatedAt: DateTime!
}
